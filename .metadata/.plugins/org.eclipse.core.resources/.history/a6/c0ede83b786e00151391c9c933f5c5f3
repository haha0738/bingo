public class Game {
	private List<Integer> _inputs = new ArrayList<>();
	private List<Player> _players = new ArrayList<>();
	
	public void start()
	{
		_players.clear();
		Scanner scanner = new Scanner(System.in);
		scanner.nextInt();
	}
}
