package com.dermot.bingo;

public class Unit {
	private int _value;
	private Matrix _matrix;
	Unit(Matrix matrix, int value)
	{
		_matrix = matrix;
		_value = value;
	}
	
	public void setValue(int value)
	{
		_value = value;
	}
	
	public int getValue()
	{
		return _value;
	}
	
	public Matrix getMatrix()
	{
		return _matrix;
	}
}
